<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:background="@color/activity_bg1"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:id="@+id/rl_bar"
        android:background="@color/colorPrimary"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize">

        <ImageView
            android:id="@+id/iv_back_cash_info"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:layout_marginStart="@dimen/default_margin10"
            android:padding="@dimen/default_margin10"
            android:src="@drawable/ic_whilte_arrow"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:lines="1"
            android:text="提现详情"
            android:textColor="@color/white"
            android:textSize="@dimen/text_size_20"/>
    </RelativeLayout>

    <LinearLayout
        android:layout_marginTop="@dimen/default_margin"
        android:padding="@dimen/default_margin10"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ImageView
            android:id="@+id/iv_state"
            android:layout_gravity="center_horizontal"
            android:src="@drawable/ic_waite"
            android:layout_width="50dp"
            android:layout_height="50dp"/>

        <TextView
            android:id="@+id/tv_cash_state"
            android:layout_marginTop="@dimen/default_margin10"
            android:gravity="center_horizontal"
            android:textColor="@color/text_color_black"
            android:textSize="@dimen/text_size_18"
            android:layout_gravity="center_vertical"
            android:text="提现中"
            android:textStyle="bold"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

        <TextView
            android:layout_marginTop="5dp"
            android:gravity="center_horizontal"
            android:textColor="@color/text_color_black_gray"
            android:textSize="@dimen/text_size_14"
            android:layout_gravity="center_vertical"
            android:text="到账时间以银行入账时间为准，请留意到账信息"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>


    </LinearLayout>


    <LinearLayout
        android:layout_marginTop="@dimen/default_margin10"
        android:padding="@dimen/default_margin15"
        android:background="@color/white"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_marginStart="@dimen/default_margin"
            android:layout_gravity="center_vertical"
            android:textSize="@dimen/text_size_16"
            android:text="姓名        "
            android:textColor="@color/text_color_black_gray"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

        <TextView
            android:id="@+id/tv_name"
            android:layout_gravity="center_vertical"
            android:layout_marginStart="@dimen/default_margin2"
            android:textSize="@dimen/text_size_16"
            android:text="周*驰"
            android:textColor="@color/text_color_black"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/line_color"/>

    <LinearLayout
        android:layout_marginTop="1dp"
        android:padding="@dimen/default_margin15"
        android:background="@color/white"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_marginStart="@dimen/default_margin"
            android:layout_gravity="center_vertical"
            android:textSize="@dimen/text_size_16"
            android:text="账户        "
            android:textColor="@color/text_color_black_gray"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

        <TextView
            android:layout_gravity="center_vertical"
            android:id="@+id/tv_alipay"
            android:layout_marginStart="@dimen/default_margin2"
            android:textSize="@dimen/text_size_16"
            android:text="支付宝（139***3381）"
            android:textColor="@color/text_color_black"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/line_color"/>

    <LinearLayout
        android:padding="@dimen/default_margin15"
        android:background="@color/white"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_marginStart="@dimen/default_margin"
            android:layout_gravity="center_vertical"
            android:textSize="@dimen/text_size_16"
            android:text="提现金额"
            android:textColor="@color/text_color_black_gray"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

        <TextView
            android:layout_gravity="center_vertical"
            android:id="@+id/tv_money"
            android:layout_marginStart="@dimen/default_margin2"
            android:textSize="@dimen/text_size_16"
            android:text="1元"
            android:textColor="@color/text_color_black"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/line_color"/>
    <LinearLayout
        android:padding="@dimen/default_margin15"
        android:background="@color/white"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_marginStart="@dimen/default_margin"
            android:layout_gravity="center_vertical"
            android:textSize="@dimen/text_size_16"
            android:text="创建时间"
            android:textColor="@color/text_color_black_gray"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

        <TextView
            android:layout_gravity="center_vertical"
            android:id="@+id/tv_time"
            android:layout_marginStart="@dimen/default_margin2"
            android:textSize="@dimen/text_size_16"
            android:text="2023-03-17 14:05:04"
            android:textColor="@color/text_color_black"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:background="@color/line_color"/>

    <LinearLayout
        android:padding="@dimen/default_margin15"
        android:background="@color/white"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_marginStart="@dimen/default_margin"
            android:layout_gravity="center_vertical"
            android:textSize="@dimen/text_size_16"
            android:text="流水号    "
            android:textColor="@color/text_color_black_gray"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

        <TextView
            android:layout_gravity="center_vertical"
            android:id="@+id/tv_msg"
            android:layout_marginStart="@dimen/default_margin2"
            android:textSize="@dimen/text_size_16"
            android:text="3242342342423"
            android:textColor="@color/text_color_black"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>

    </LinearLayout>

</LinearLayout>